
@{

    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    
    <meta name="viewport" content="width=device-width" />

    <title>NET CORE 2.x - @ViewBag.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
</head>
<body>
    <nav class="navbar navbar-inverse">
        <div class="container">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index">NET CORE 2.x</a>
            <ul class="navbar-brand d-flex justify-content-around">
                <li class="active"><a asp-controller="Home" asp-action="Index">Trang chủ</a></li>
                <li class=""><a asp-controller="Home" asp-action="About">Giới Thiệu</a></li>
                <li class=""><a asp-controller="Home" asp-action="Contact">Liên Hệ</a></li>
                <li class=""><a asp-controller="Book" asp-action="Index">Sách</a></li>

            </ul>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
    <!-- jQuery library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    @RenderSection("Scripts",required:false)
</body>
</html>
