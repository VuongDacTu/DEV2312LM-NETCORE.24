
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Lab07</title>
    <link rel="stylesheet" href="~/lib/bootstrap/~/dist/css/bootstrap.min.css" />
    @RenderSection("Css",required:false)
</head>
<body>
    <header>
        <nav class="navbar navbar-inverse">
            <div class="container">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a  asp-area="" asp-controller="Home" asp-action="Index">Trang chủ</a>
                        </li>
                        <li class="nav-item">
                            <a  asp-area="" asp-controller="Home" asp-action="About">Giới thiệu</a>
                        </li>
                    <li class="nav-item">
                        <a asp-area="" asp-controller="Home" asp-action="Contact">Liên hệ</a>
                    </li>
                    </ul>

            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Lab07 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/~/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/~/dist/js/bootstrap.bundle.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
