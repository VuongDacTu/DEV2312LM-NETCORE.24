
@{
    ViewData["Title"] = "GetListMember";
}

<h1>Danh sách thành viên</h1>
<a class="" asp-area="" asp-controller="Member" asp-action="Create">Create</a>

<table class="table table-bordered">
    <tr>
        <th>Member Id</th>
        <th>Full Name</th>
        <th>User Name</th>
        <th>Password</th>
        <th>Email</th>
    </tr>
    @foreach(var item in @ViewBag.members)
    {
        <tr>
            <td>
                @item.MemberId
            </td>
            <td>
                @item.FullName
            </td>
            <td>
                @item.UserName
            </td>
            <td>
                @item.Password
            </td>
            <td>
                @item.Email
            </td>
        </tr>
    }
</table>
